@model List<Usuario>
<h1>Usuario Cadastrado no Sistema</h1>
<p class="text-danger">@ViewData["Menssagem"]</p>

<table class="table">
    <thead>
        <tr>
            <th scope="row">Nome</th>
            <th scope="row">Login</th>
            <th scope="row">Tipo</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Usuario u in Model)
        {
            <tr>
                <td>@u.Nome</td>
                <td>@u.Login</td>
                @if (u.Tipo == Usuario.ADMIN)
                {
                    <td>Administrador</td>
                }
                else
                {
                    <td>Padr√£o</td>
                }
                <td><a href="editarUsuario?id=@u.Id">Editar</a></td>
                <td><a href="ExcluirUsuario?id=@u.Id">Excluir</a></td>
            </tr>
        }
    </tbody>
</table>
<a href="RegistrarUsuarios">Registrar Novo Usuario</a>